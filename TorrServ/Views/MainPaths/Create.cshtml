@model TorrServData.Models.SourceOfMovies
@{
    ViewBag.Title = "Add Path";
}

<form asp-action="Create" asp-controller="MainPaths">
    <div asp-validation-summary="All" class="btn-dark"></div>
    <div class="form-group btn-dark">
        <label asp-for="path" class="custom-control-label">Path</label>
        <input type="text" asp-for="path" class="form-control" />
    </div>    
    <div class="form-group btn-dark">
        <input type="submit" value="Add" class="btn btn-secondary" />
    </div>
</form>

